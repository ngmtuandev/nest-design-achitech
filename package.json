{
  "name": "cell-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@azure-rest/ai-document-intelligence": "^1.0.0-beta.2",
    "@azure/ai-form-recognizer": "^5.0.0",
    "@azure/identity": "^4.0.1",
    "@azure/storage-blob": "^12.17.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.1.2",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.1.2",
    "@socket.io/redis-adapter": "^8.2.1",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fuse.js": "^7.0.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.36.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
    "nestjs-typeorm-paginate": "^4.0.4",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "serve-static": "^1.15.0",
    "socket.io": "^4.7.1",
    "typeorm": "^0.3.20",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/serve-static": "^1.15.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/",
      "<rootDir>/core/",
      "<rootDir>/infrastructure/",
      "<rootDir>/integration/"
    ],
    "moduleNameMapper": {
      "^@core/domain(|/.*)$": "<rootDir>/core/domain/src/$1",
      "^@core/domain-business-card(|/.*)$": "<rootDir>/core/domain-business-card/src/$1",
      "^@core/domain-ngword(|/.*)$": "<rootDir>/core/domain-ngword/src/$1",
      "^@core/application(|/.*)$": "<rootDir>/core/application/src/$1",
      "^@core/application-business-card(|/.*)$": "<rootDir>/core/application-business-card/src/$1",
      "^@core/application-ngword(|/.*)$": "<rootDir>/core/application-ngword/src/$1",
      "^@infrastructure/middleware(|/.*)$": "<rootDir>/infrastructure/middleware/src/$1",
      "^@infrastructure/repository(|/.*)$": "<rootDir>/infrastructure/repository/src/$1",
      "^@integration/amqp-gateway(|/.*)$": "<rootDir>/integration/amqp-gateway/src/$1",
      "^@integration/socket-gateway(|/.*)$": "<rootDir>/integration/socket-gateway/src/$1",
      "^@integration/api-gateway(|/.*)$": "<rootDir>/integration/api-gateway/src/$1",
      "^@infrastructure/xlogger(|/.*)$": "<rootDir>/infrastructure/xlogger/src/$1",
      "^@infrastructure/xhelper(|/.*)$": "<rootDir>/infrastructure/xhelper/src/$1",
      "^@infrastructure/message(|/.*)$": "<rootDir>/infrastructure/message/src/$1"
    }
  }
}
